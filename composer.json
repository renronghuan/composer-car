{
    "name": "renronghuan/composer-car",
    "type": "library",
    "description": "In order to study composer",
    "require": {
             "renronghuan/composer-test": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Ford\\": "src"           
        },
	"psr-4": {
